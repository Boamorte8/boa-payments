{
  "name": "boa-payments",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "format": "prettier .  --write",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "vitest --environment jsdom",
    "test:unitcommit": "vitest --run --environment jsdom",
    "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "commit": "commit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.1",
    "pinia": "^2.0.14",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.35",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/prompt-cli": "^16.2.4",
    "@intlify/eslint-plugin-vue-i18n": "^1.4.1",
    "@pinia/testing": "^0.0.12",
    "@rushstack/eslint-patch": "^1.1.3",
    "@storybook/addon-actions": "^6.5.0-beta.7",
    "@storybook/addon-essentials": "^6.5.0-beta.7",
    "@storybook/addon-links": "^6.5.0-beta.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.5.0-beta.7",
    "@types/fetch-mock": "^7.3.5",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.31",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.19",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "fetch-mock": "^9.11.0",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "msw": "^0.39.2",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "storybook-builder-vite": "^0.1.23",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "vite": "^2.9.8",
    "vitest": "^0.12.3",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.34.11",
    "whatwg-fetch": "^3.6.2"
  }
}
