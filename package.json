{
  "name": "boa-payments",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "format": "prettier .  --write",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "vitest --config ./vitest.config.ts --environment jsdom",
    "test:unitui": "vitest --config ./vitest.config.ts --ui --environment jsdom",
    "test:unitcommit": "vitest --config ./vitest.config.ts --run --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "commit": "commit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.7",
    "@vuepic/vue-datepicker": "^3.4.1",
    "@vueuse/core": "^9.1.0",
    "pinia": "^2.0.18",
    "vue": "^3.2.37",
    "vue-currency-input": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@pinia/testing": "^0.0.13",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.10",
    "@types/fetch-mock": "^7.3.5",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.2",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/ui": "^0.21.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "fetch-mock": "^9.11.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "msw": "^0.44.2",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "storybook-dark-mode": "^1.1.0",
    "storybook-vue3-router": "^2.3.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "vite-plugin-pwa": "^0.12.3",
    "vitest": "^0.21.1",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.40.1",
    "whatwg-fetch": "^3.6.2",
    "workbox-build": "^6.5.4",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-core": "^6.5.4",
