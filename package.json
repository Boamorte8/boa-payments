{
  "name": "boa-payments",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "format": "prettier .  --write",
    "prepare": "husky install",
    "test:unit": "vitest --config ./vitest.config.ts --environment jsdom",
    "test:unitui": "vitest --config ./vitest.config.ts --ui --environment jsdom",
    "test:unitcommit": "vitest --config ./vitest.config.ts --run --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "commit": "commit"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.12",
    "@intlify/unplugin-vue-i18n": "0.10.0",
    "@vuepic/vue-datepicker": "4.2.3",
    "@vueuse/core": "9.13.0",
    "pinia": "2.0.33",
    "vue": "3.2.47",
    "vue-currency-input": "3.0.3",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@commitlint/prompt-cli": "17.5.0",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@pinia/testing": "0.0.15",
    "@rushstack/eslint-patch": "1.2.0",
    "@types/fetch-mock": "7.3.5",
    "@types/jsdom": "21.1.1",
    "@types/node": "18.15.10",
    "@vitejs/plugin-vue": "4.1.0",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vitest/ui": "0.29.7",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.3.2",
    "@vue/tsconfig": "0.1.3",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "c8": "7.13.0",
    "cross-fetch": "3.1.5",
    "eslint": "8.36.0",
    "eslint-plugin-vue": "9.10.0",
    "fetch-mock": "9.11.0",
    "husky": "8.0.3",
    "jsdom": "21.1.1",
    "msw": "1.2.1",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "tailwindcss": "3.2.7",
    "typescript": "5.0.2",
    "vite": "4.2.1",
    "vite-plugin-pwa": "0.14.6",
    "vitest": "0.29.7",
    "vue-loader": "17.0.1",
    "vue-tsc": "1.2.0",
    "workbox-build": "6.5.4",
    "workbox-cacheable-response": "6.5.4",
    "workbox-core": "6.5.4",
    "workbox-precaching": "6.5.4",
    "workbox-routing": "6.5.4",
    "workbox-strategies": "6.5.4"
  }
}
