{
  "name": "boa-payments",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "format": "prettier .  --write",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "vitest --environment jsdom",
    "test:unitcommit": "vitest --run --environment jsdom",
    "typecheck": "vue-tsc --noEmit && vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "commit": "commit"
  },
  "dependencies": {
    "@headlessui/vue": "^1.5.0",
    "pinia": "^2.0.13",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.34",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.3",
    "@intlify/eslint-plugin-vue-i18n": "^1.4.1",
    "@pinia/testing": "^0.0.11",
    "@rushstack/eslint-patch": "^1.1.2",
    "@storybook/addon-actions": "^6.5.0-alpha.60",
    "@storybook/addon-essentials": "^6.5.0-alpha.60",
    "@storybook/addon-links": "^6.5.0-alpha.60",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/vue3": "^6.5.0-alpha.60",
    "@types/fetch-mock": "^7.3.5",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "2.0.0-rc.19",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^8.6.0",
    "fetch-mock": "^9.11.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "msw": "^0.39.2",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "storybook-builder-vite": "^0.1.23",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.9.2",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.33.9",
    "whatwg-fetch": "^3.6.2"
  }
}
