{
  "name": "boa-payments",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "format": "prettier .  --write",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:unit": "vitest --config ./vitest.config.ts --environment jsdom",
    "test:unitui": "vitest --config ./vitest.config.ts --ui --environment jsdom",
    "test:unitcommit": "vitest --config ./vitest.config.ts --run --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "commit": "commit"
  },
  "dependencies": {
    "@headlessui/vue": "1.7.9",
    "@intlify/unplugin-vue-i18n": "0.8.2",
    "@vuepic/vue-datepicker": "3.6.8",
    "@vueuse/core": "9.12.0",
    "pinia": "2.0.30",
    "vue": "3.2.47",
    "vue-currency-input": "3.0.3",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@commitlint/prompt-cli": "17.4.2",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@pinia/testing": "0.0.14",
    "@rushstack/eslint-patch": "1.2.0",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-vite": "0.3.0",
    "@storybook/testing-library": "0.0.13",
    "@storybook/vue3": "6.5.16",
    "@types/fetch-mock": "7.3.5",
    "@types/jsdom": "21.1.0",
    "@types/node": "18.13.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "@vitest/ui": "0.28.4",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.2.10",
    "@vue/tsconfig": "0.1.3",
    "autoprefixer": "10.4.13",
    "babel-loader": "8.2.5",
    "c8": "7.12.0",
    "cross-fetch": "3.1.5",
    "eslint": "8.34.0",
    "eslint-plugin-vue": "9.9.0",
    "fetch-mock": "9.11.0",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "msw": "1.0.1",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "storybook-dark-mode": "2.0.6",
    "storybook-vue3-router": "2.3.1",
    "tailwindcss": "3.2.6",
    "typescript": "4.9.5",
    "vite": "4.0.4",
    "vite-plugin-pwa": "0.14.4",
    "vitest": "0.28.4",
    "vue-loader": "17.0.1",
    "vue-tsc": "1.0.24",
